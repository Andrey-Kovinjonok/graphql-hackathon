{
  "env": {
    "node": true,
    "mocha": true,
  },
  "parser": "babel-eslint",
  "rules": {
    "global-require": 0,
    "no-param-reassign": 0,
    "quote-props": 0,
    "array-callback-return": 0,
    "operator-assignment": 0,
    "arrow-parens": 0,
    "no-shadow": 0,
    "no-unneeded-ternary": 0,
    "no-unused-vars": 0,
    "consistent-return": 0,
    "new-cap": 0,
    "brace-style": 0,
    "func-names": 0,
    "indent": 0,
    "no-constant-condition": 0,
    "comma-dangle": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-new": 0,
    "camelcase": 0,
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "import/imports-first": 0,
    "prefer-template": 0
  }
}
